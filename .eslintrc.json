{
    "extends": "trendmicro",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "arrow-body-style": ["error", "always"],
        "implicit-arrow-linebreak": ["error", "beside"]
    }
}